SELECT  MONTH(LOGDATE) AS MONTH,SUM(NBR_OF_CALLS) AS CICS_H_TRANS
FROM U1DPH0.TXCDLOG	
WHERE LOGDATE >= '2020-01-01'
AND LOGDATE <='2020-09-30'
AND IPCLIENT <> 'EUPWREQ_1'
AND IASVERSION = '  '
AND USERID IN ('AGSKMIG',  'BGSKMIG', 'CGSKMIG', 'DGSKMIG', 'EGSKMIG', 'FGSKMIG', 'GGSKMIG', 'HGSKMIG', 'IGSKMIG', 'KGSKMIG' , 'SGSKMIG')
GROUP BY MONTH(LOGDATE)
WITH UR;

